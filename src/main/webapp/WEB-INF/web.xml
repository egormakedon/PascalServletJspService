<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>PascalServletJspService</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <error-page>
        <location>/error/errorPage.jsp</location>
    </error-page>

    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <servlet-name>PascalWebServlet</servlet-name>
    </filter-mapping>

    <filter-mapping>
        <filter-name>LastQuerySaverFilter</filter-name>
        <servlet-name>PascalWebServlet</servlet-name>
    </filter-mapping>

    <filter-mapping>
        <filter-name>DropRequestFilter</filter-name>
        <servlet-name>PascalWebServlet</servlet-name>
    </filter-mapping>

    <filter-mapping>
        <filter-name>TakeTitlesFilter</filter-name>
        <servlet-name>PascalWebServlet</servlet-name>
    </filter-mapping>
</web-app>